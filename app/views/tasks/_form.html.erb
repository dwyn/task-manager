<%= form_for(@task) do |f| %>
  <%= f.hidden_field :user_id %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.hidden_field :start_time %>
  <%= f.fields_for :category do |build| %>
    <%= build.label 'Category Name' %>
    <%= build.text_field :name %>
  <% end %>
  <%= f.submit %>
<% end %>